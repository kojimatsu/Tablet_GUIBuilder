<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/abstract_widget_scrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/fragment_frame"
    tools:context="jp.ac.shibaura_it.se.sayo.guibuilder.MainActivity$PlaceholderFragment">

    <LinearLayout
        android:id="@+id/selection_widgets"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:text="Layout"
            android:textSize="20dp"
            android:background="#7d7f80"
            android:textColor="#ffffff"
            android:layout_marginBottom="5dp" />


        <LinearLayout
            android:id="@+id/layout_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone">

            <LinearLayout
                android:id="@+id/_stack"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Stack"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_grid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Grid"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_scroll"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Scroll"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </LinearLayout>

        </LinearLayout>

        <TextView
            android:id="@+id/_inputWidget"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:text="InputWidget"
            android:textSize="20dp"
            android:background="#7d7f80"
            android:textColor="#ffffff"
            android:layout_marginBottom="5dp" />

        <LinearLayout
            android:id="@+id/inputwidget_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone">

            <LinearLayout
                android:id="@+id/_button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:text="Button" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_switch"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <Switch
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:text="Switch" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_editText"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:duplicateParentState="false"
                    android:ems="10"
                    android:enabled="false"
                    android:fitsSystemWindows="false"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:freezesText="false"
                    android:hint="TextBox"
                    android:includeFontPadding="false"
                    android:linksClickable="false"
                    android:longClickable="false"
                    android:scrollHorizontally="false"
                    android:scrollbarAlwaysDrawVerticalTrack="false"
                    android:selectAllOnFocus="false"
                    android:textAllCaps="false" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_seekber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/seekber" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_datePicker"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="DatePicker"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_timePicker"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="TimePicker"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_spinner"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <Spinner
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:entries="@array/spinner" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_radioButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:text="RadioButton" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_checkBox"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:text="CheckBox" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_tab"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Tab"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </LinearLayout>
        </LinearLayout>

        <TextView
            android:id="@+id/_outputWidget"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:text="OutputWidget"
            android:textSize="20dp"
            android:background="#7d7f80"
            android:textColor="#ffffff" />


        <LinearLayout
            android:id="@+id/outputwidget_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone">

            <LinearLayout
                android:id="@+id/_label"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Label"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_imageView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@android:drawable/sym_def_app_icon" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ImageView"
                    android:textAppearance="?android:attr/textAppearanceMedium" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_mediaPlayer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="MediaPlayer"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_dialog"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Dialog"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/view_pressed_false"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="List"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</ScrollView>